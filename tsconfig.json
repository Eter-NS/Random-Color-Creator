{
	"compilerOptions": {
		"forceConsistentCasingInFileNames": true,
		"target": "ES2016",
		"useDefineForClassFields": true,
		"module": "ES2015",
		"lib": ["ES2016", "DOM"],
		"moduleResolution": "Node",
		"strict": true,
		"resolveJsonModule": true,
		"isolatedModules": true,
		"esModuleInterop": true,
		"noEmit": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noImplicitReturns": true,
		"skipLibCheck": true
	},
	"include": ["src"]
}
